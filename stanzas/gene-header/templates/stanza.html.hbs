{{#with result}}
<table class="table header-xref gene-header">
  <thead>
  <tr>
    {{#each xrefs}}
    <th>{{name}}</th>
    {{/each}}
  </tr>
  </thead>
  <tbody>
  <tr>
    {{#each xrefs}}
    <td>
      {{#each refs}}
        {{this.label}},
      {{/each}}
      {{#each approved_name}}
        {{this.label}}
      {{/each}}
    </td>
    {{/each}}
  </tr>
  </tbody>
</table>
{{/with}}
